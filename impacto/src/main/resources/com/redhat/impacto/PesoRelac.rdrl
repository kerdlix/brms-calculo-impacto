package com.redhat.impacto;

import java.lang.Number;

rule "PesoRelac"
	dialect "mvel"
	ruleflow-group "pesoRelac"
	when
		$c : Calculo( _totalReceb > 0.0 )
	then
		$c.set_pesoRelac( $c.getPagamento() / $c.get_totalReceb() );
		$c.set_impacto( ($c.getScoreOrigem() * $c.getPagamento() ) / $c.get_totalReceb() );
		System.out.println("CALCULO IMPACTO: " + $c.get_impacto());
end
