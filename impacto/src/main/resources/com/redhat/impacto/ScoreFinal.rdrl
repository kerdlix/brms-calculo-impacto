package com.redhat.impacto;

import java.lang.Number;

rule "ScoreFinal"
	dialect "mvel"
	no-loop true
	lock-on-active true
	ruleflow-group "scoreFinal"
	when
		$c : Calculo( )
	then
		modify( $c ) {
				set_scoreParcial( ($c.getScoreDestino() * $c.get_fatorAbsorcao()) + ($c.get_impacto() * $c.get_fatorImpacto()) )
		}
end
