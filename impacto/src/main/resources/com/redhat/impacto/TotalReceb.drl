package com.redhat.impacto;

rule "Calculo do Total Receb."
no-loop 
    
    when
        $c : Calculo()
        $total : Double() from accumulate (Recebimento($r : recebimento) from $c.recebimentos, sum($r)) 
    then
        $c.set_totalReceb($total); 
        System.out.println("CALCULO TOTAL RECEB: " + $total);
    end    
    